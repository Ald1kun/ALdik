package com.company ;
import java.io.PrintStream;
import java.util.Random;
import java.util.Scanner;
public class Main{


    public static void main(String[] args) {
          Scanner sc=new Scanner(System.in);
        Student A = new Student(); // создание объекта
        System.out.println("VVedite kolichestvo studentov");
        int n = sc.nextInt();
        for (int i = 0;i<=n;i++){A.Info();}

    }
}
class Student{

    String name;    // имя
    int age;        // возраст
    String Facultet;//Факультет
    String Group;//Группа
    void Info(){
       System.out.print( " + namepy" +"\n" + "Student in facultet:" + Facultet + "\n" + "in group: " + Group + "Here is:" + age);
    }
}
